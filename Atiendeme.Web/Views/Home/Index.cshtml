@{
    ViewData["Title"] = "Home Page";
}

<!-- start banner Area -->
<section class="home-banner-area relative" id="home" data-parallax="scroll" data-image-src="/assets/images/odontologia.jpg">
    <div class="overlay-bg overlay"></div>
    <h1 class="template-name">ATIENDEME</h1>
    <div class="container">
        <div class="row fullscreen">
            <div class="banner-content col-lg-12">
                <p>Construyendo sonrisas</p>
                <h1>
                    Tu sonrisa,<br> nuestro objetivo.
                </h1>
            </div>
        </div>
    </div>
    <div class="head-bottom-meta">
        <div class="d-flex meta-left no-padding">
            <a href="#"><span class="fab fa-facebook-f icon-fontawesome-service"></span></a>
            <a href="#"><span class="fab fa-twitter icon-fontawesome-service"></span></a>
            <a href="#"><span class="fab fa-instagram icon-fontawesome-service"></span></a>
        </div>
    </div>
</section>
<!-- End banner Area -->
<!-- Start features Area -->

<div class="pt-5"  id="servicios">
    <div class="col-md-12">
        <h2 class="text-center">NUESTROS SERVICIOS</h2>
    </div>
</div>

<section class="features-area section-gap-top">
    <div class="container">

        <div class="row feature_inner">
            <div class="col-lg-3 col-md-6">
                <div class="feature-item">
                    <i class="fas fa-crown icon-fontawesome-service"></i>
                    <h4>Coronas Dentales</h4>
                </div>
            </div>
            <div class="col-lg-3 col-md-6">
                <div class="feature-item">
                    <i class="far fa-grimace icon-fontawesome-service"></i>
                    <h4>Implantes Dentales</h4>
                </div>
            </div>
            <div class="col-lg-3 col-md-6">
                <div class="feature-item">
                    <i class="fas fa-teeth-open icon-fontawesome-service"></i>
                    <h4>Blanqueamiento</h4>
                </div>
            </div>
            <div class="col-lg-3 col-md-6">
                <div class="feature-item">
                    <i class="fas fa-tooth icon-fontawesome-service"></i>
                    <h4>Extracción</h4>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End features Area -->
<!-- Start About Area -->
<section class="about-area section-gap"  id="about-us"style="padding: 120px">
    <div class="container">
        <div class="row align-items-center justify-content-center">
            <div class="col-lg-7 col-md-12 about-left">
                <img class="img-fluid" src="~/assets/images/dentista-nina.jpg" alt="">
            </div>
            <div class="col-lg-5 col-md-12 about-right">
                <div class="section-title text-left">
                    <h4>Sobre Nosotros</h4>
                    <h2 style="font-size: 36px">Somos una compañida<br /> comprometida contigo</h2>
                </div>
                <div>
                    <p>
                        Somos una empresa enfocada en el trabajo honesto, llevando siempre la mejor calidad de servicio que se pueda otorgar.
                    </p>
                </div>
            </div>
        </div>
    </div>
</section>
<!-- End About Area -->
